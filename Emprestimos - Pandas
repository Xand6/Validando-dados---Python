#Concatenas todos CSV em apenas um Ãºnico e apaga lista_emprestimos
emprestimos = pandas.concat([pandas.read_csv(emprestimo) for emprestimo in lista_emprestimos], ignore_index=True)
del lista_emprestimos

#Exclui linhas que possuem o mesmo valor na coluna 'id_emprestimo' do novo CSV
emprestimos = emprestimos.drop_duplicates(subset=['id_emprestimo'])
